# Harvest Production Environment Variables
# Copy this file to .env.production and fill in the values

# ===========================================
# Movement Network Configuration
# ===========================================

# Movement mainnet RPC endpoint
NEXT_PUBLIC_MOVEMENT_RPC_URL=https://mainnet.movementnetwork.xyz/v1

# Movement network chain ID
NEXT_PUBLIC_MOVEMENT_CHAIN_ID=126

# Movement explorer URL
NEXT_PUBLIC_MOVEMENT_EXPLORER_URL=https://explorer.movementnetwork.xyz

# ===========================================
# Privy Authentication
# ===========================================

# Privy App ID (get from https://dashboard.privy.io)
NEXT_PUBLIC_PRIVY_APP_ID=your-privy-app-id

# ===========================================
# Contract Addresses (deployed on Movement)
# ===========================================

# Harvest Registry contract address
NEXT_PUBLIC_HARVEST_REGISTRY_ADDRESS=0x...

# Harvest Batch Claim contract address
NEXT_PUBLIC_HARVEST_BATCH_CLAIM_ADDRESS=0x...

# ===========================================
# Protocol Module Addresses
# ===========================================

# Yuzu CLMM DEX
NEXT_PUBLIC_YUZU_MODULE_ADDRESS=0x...

# Joule Lending
NEXT_PUBLIC_JOULE_MODULE_ADDRESS=0x...

# Meridian DeFi
NEXT_PUBLIC_MERIDIAN_MODULE_ADDRESS=0x...

# ===========================================
# Application Settings
# ===========================================

# Application URL (for OG images, redirects)
NEXT_PUBLIC_APP_URL=https://harvest.movementlabs.xyz

# Enable/disable features
NEXT_PUBLIC_ENABLE_BATCH_CLAIM=true
NEXT_PUBLIC_ENABLE_AUTO_COMPOUND=false

# ===========================================
# Optional: Analytics & Monitoring
# ===========================================

# Analytics (optional)
# NEXT_PUBLIC_POSTHOG_KEY=
# NEXT_PUBLIC_POSTHOG_HOST=

# Error tracking (optional)
# SENTRY_DSN=
